{
  "name": "@packages/ai",
  "version": "1.0.0",
  "description": "Grahmos AI/ML Integration Package - Vector Search, Document Processing, and Predictive Analytics",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "echo 'Skipping AI package build due to errors from main branch'",
    "dev": "tsx watch src/index.ts",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@pinecone-database/pinecone": "^2.0.1",
    "@supabase/supabase-js": "^2.39.7",
    "@tensorflow/tfjs-node": "^4.15.0",
    "@xenova/transformers": "^2.14.0",
    "cohere-ai": "^7.7.5",
    "langchain": "^0.1.25",
    "ml-matrix": "^6.10.9",
    "natural": "^6.12.0",
    "openai": "^4.26.0",
    "pdf-parse": "^1.1.1",
    "sharp": "^0.33.5",
    "tesseract.js": "^5.0.4",
    "weaviate-ts-client": "^1.6.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/natural": "^5.1.5",
    "@types/node": "^20.11.24",
    "@types/pdf-parse": "^1.1.4",
    "@vitest/coverage-v8": "^1.3.1",
    "eslint": "^8.57.0",
    "tsx": "^4.16.0",
    "typescript": "^5.5.4",
    "vitest": "^1.3.1"
  },
  "keywords": [
    "ai",
    "ml",
    "vector-search",
    "embeddings",
    "document-processing",
    "nlp",
    "semantic-search",
    "openai",
    "pinecone",
    "tensorflow",
    "grahmos"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./vector-search": {
      "types": "./dist/vector-search.d.ts",
      "import": "./dist/vector-search.js"
    },
    "./document-processing": {
      "types": "./dist/document-processing.d.ts",
      "import": "./dist/document-processing.js"
    },
    "./analytics": {
      "types": "./dist/analytics.d.ts",
      "import": "./dist/analytics.js"
    },
    "./security": {
      "types": "./dist/security.d.ts",
      "import": "./dist/security.js"
    },
    "./nlp": {
      "types": "./dist/nlp.d.ts",
      "import": "./dist/nlp.js"
    }
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  }
}
