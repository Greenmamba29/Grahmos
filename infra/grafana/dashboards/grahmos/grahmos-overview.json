{
  "dashboard": {
    "id": null,
    "uid": "grahmos-overview",
    "title": "Grahmos - Application Overview",
    "description": "Comprehensive overview of Grahmos PWA performance, user engagement, and system health",
    "tags": ["grahmos", "pwa", "overview", "analytics"],
    "timezone": "browser",
    "editable": true,
    "schemaVersion": 39,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "System Health Summary",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"grahmos-pwa\"}",
            "legendFormat": "PWA Status",
            "refId": "A"
          },
          {
            "expr": "up{job=\"grahmos-assistant\"}",
            "legendFormat": "Assistant Status",
            "refId": "B"
          },
          {
            "expr": "up{job=\"grahmos-edge-api\"}",
            "legendFormat": "API Status",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN"
                  },
                  "1": {
                    "text": "UP"
                  }
                },
                "type": "value"
              }
            ],
            "unit": "short"
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "horizontal",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center"
        },
        "pluginVersion": "10.2.0",
        "gridPos": {
          "h": 4,
          "w": 24,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Page Views (24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(grahmos_page_views_total[24h]))",
            "legendFormat": "Total Page Views",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "fixedColor": "blue",
              "mode": "fixed"
            },
            "unit": "short"
          }
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "horizontal"
        },
        "gridPos": {
          "h": 6,
          "w": 4,
          "x": 0,
          "y": 4
        }
      },
      {
        "id": 3,
        "title": "Active Sessions",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(assistant_active_sessions)",
            "legendFormat": "Active Sessions",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "fixedColor": "green",
              "mode": "fixed"
            },
            "unit": "short"
          }
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "horizontal"
        },
        "gridPos": {
          "h": 6,
          "w": 4,
          "x": 4,
          "y": 4
        }
      },
      {
        "id": 4,
        "title": "Search Requests (1h)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(grahmos_searches_total[1h]))",
            "legendFormat": "Search Requests",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "fixedColor": "orange",
              "mode": "fixed"
            },
            "unit": "short"
          }
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "horizontal"
        },
        "gridPos": {
          "h": 6,
          "w": 4,
          "x": 8,
          "y": 4
        }
      },
      {
        "id": 5,
        "title": "AI Interactions (1h)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(grahmos_assistant_interactions_total[1h]))",
            "legendFormat": "AI Interactions",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "fixedColor": "purple",
              "mode": "fixed"
            },
            "unit": "short"
          }
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "horizontal"
        },
        "gridPos": {
          "h": 6,
          "w": 4,
          "x": 12,
          "y": 4
        }
      },
      {
        "id": 6,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(grahmos_errors_total[5m])) * 100",
            "legendFormat": "Error Rate %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            },
            "unit": "percent"
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "horizontal"
        },
        "gridPos": {
          "h": 6,
          "w": 4,
          "x": 16,
          "y": 4
        }
      },
      {
        "id": 7,
        "title": "Avg Response Time",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(rate(grahmos_operation_duration_sum[5m]) / rate(grahmos_operation_duration_count[5m]))",
            "legendFormat": "Avg Response Time",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 500
                },
                {
                  "color": "red",
                  "value": 1000
                }
              ]
            },
            "unit": "ms"
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "horizontal"
        },
        "gridPos": {
          "h": 6,
          "w": 4,
          "x": 20,
          "y": 4
        }
      },
      {
        "id": 8,
        "title": "Page Views Over Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(grahmos_page_views_total[5m])) * 60",
            "legendFormat": "Page Views/min",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 20,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "unit": "reqps"
          }
        },
        "options": {
          "legend": {
            "calcs": [],
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "single",
            "sort": "none"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 10
        }
      },
      {
        "id": 9,
        "title": "User Engagement Metrics",
        "type": "timeseries",
        "targets": [
          {
            "expr": "avg(grahmos_user_engagement_total{engagement_session_duration!=\"\"})",
            "legendFormat": "Session Duration (avg)",
            "refId": "A"
          },
          {
            "expr": "avg(grahmos_user_engagement_total{engagement_clicks!=\"\"})",
            "legendFormat": "Clicks per Session (avg)",
            "refId": "B"
          },
          {
            "expr": "avg(grahmos_user_engagement_total{engagement_scroll_depth!=\"\"})",
            "legendFormat": "Scroll Depth % (avg)",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 20,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "unit": "short"
          }
        },
        "options": {
          "legend": {
            "calcs": [],
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 10
        }
      },
      {
        "id": 10,
        "title": "Search Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(grahmos_operation_duration_bucket{operation=\"search\"}[5m])) by (le))",
            "legendFormat": "50th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(grahmos_operation_duration_bucket{operation=\"search\"}[5m])) by (le))",
            "legendFormat": "95th percentile",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(grahmos_operation_duration_bucket{operation=\"search\"}[5m])) by (le))",
            "legendFormat": "99th percentile",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "Response Time",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 20,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "unit": "ms"
          }
        },
        "options": {
          "legend": {
            "calcs": [],
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 18
        }
      },
      {
        "id": 11,
        "title": "AI Assistant Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(assistant_llm_requests_total[5m])) by (llm_model)",
            "legendFormat": "{{ llm_model }} Requests/sec",
            "refId": "A"
          },
          {
            "expr": "avg(rate(assistant_llm_response_duration_sum[5m]) / rate(assistant_llm_response_duration_count[5m])) by (llm_model)",
            "legendFormat": "{{ llm_model }} Avg Response Time",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 20,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "unit": "short"
          }
        },
        "options": {
          "legend": {
            "calcs": [],
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 18
        }
      },
      {
        "id": 12,
        "title": "Feature Usage Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (feature_name) (grahmos_feature_usage_total)",
            "legendFormat": "{{ feature_name }}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "tooltip": false,
                "vis": false,
                "legend": false
              }
            },
            "mappings": []
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "pieType": "pie",
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "displayMode": "list",
            "placement": "right",
            "values": []
          },
          "displayLabels": ["name", "percent"]
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 26
        }
      },
      {
        "id": 13,
        "title": "Top Error Types",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum by (error_type, error_context) (increase(grahmos_errors_total[24h])))",
            "legendFormat": "{{ error_type }} - {{ error_context }}",
            "refId": "A",
            "format": "table"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 10
                },
                {
                  "color": "red",
                  "value": 50
                }
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "showHeader": true
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true
              },
              "indexByName": {},
              "renameByName": {
                "error_context": "Context",
                "error_type": "Error Type",
                "Value": "Count"
              }
            }
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 26
        }
      },
      {
        "id": 14,
        "title": "Core Web Vitals",
        "type": "timeseries",
        "targets": [
          {
            "expr": "avg(grahmos_core_web_vitals_total{metric=\"lcp\"})",
            "legendFormat": "Largest Contentful Paint (LCP)",
            "refId": "A"
          },
          {
            "expr": "avg(grahmos_core_web_vitals_total{metric=\"fid\"})",
            "legendFormat": "First Input Delay (FID)",
            "refId": "B"
          },
          {
            "expr": "avg(grahmos_core_web_vitals_total{metric=\"cls\"})",
            "legendFormat": "Cumulative Layout Shift (CLS)",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "Time (ms) / Score",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 20,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "line"
              }
            },
            "thresholds": {
              "steps": [
                {
                  "color": "transparent"
                },
                {
                  "color": "red",
                  "value": 2500
                }
              ]
            },
            "unit": "ms"
          }
        },
        "options": {
          "legend": {
            "calcs": [],
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 26
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "environment",
          "type": "query",
          "query": "label_values(up, environment)",
          "refresh": 1,
          "includeAll": false,
          "multi": false,
          "options": [],
          "current": {
            "selected": false,
            "text": "production",
            "value": "production"
          },
          "hide": 0,
          "skipUrlSync": false
        },
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(up{environment=\"$environment\"}, instance)",
          "refresh": 1,
          "includeAll": true,
          "multi": true,
          "options": [],
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "hide": 0,
          "skipUrlSync": false
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          },
          "enable": true,
          "expr": "changes(up{job=\"grahmos-pwa\"}[5m]) > 0",
          "iconColor": "green",
          "textFormat": "Deployment event",
          "titleFormat": "Deployment"
        }
      ]
    }
  }
}
